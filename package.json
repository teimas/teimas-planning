{
  "name": "planning-teimas",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "author": "Ludo Bermejo <ludo.bermejo@teimas.com>",
  "description": "Multiplatform planning application by Teimas",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "setup-env": "node ./scripts/setup-env.js"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["./jest.setup.js", "./__tests__/jest.setup.js"],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-navigation|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/vendor/**",
      "!**/__tests__/**",
      "!**/hooks/useColorScheme.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.{ts,tsx}"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.2.1",
    "expo": "~52.0.39",
    "expo-blur": "~14.0.3",
    "expo-clipboard": "^7.0.1",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-localization": "^16.0.1",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "firebase": "^11.4.0",
    "i18next": "^24.2.3",
    "mime-db": "^1.54.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/parser": "^7.26.10",
    "@testing-library/react-native": "^13.2.0",
    "@types/babel__traverse": "^7.20.6",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "expo-sharing": "^13.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "prettier": "^3.5.3",
    "react-test-renderer": "18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "private": true
}
